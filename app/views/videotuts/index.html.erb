										  <tr>
											<td><span id="video-tutorial" style="color:#368EE0; font-weight: bold;"><%= link_to videotut.title, lecture_videotut_path(videotut.tutorial, videotut), :target => '_blank' %></span></td>
                                            
										  </tr>
										<% end %>
										</table>
										</div>
					
				  </div>
					<div class="span2">
					 <div class="ad-right-tower">
					   <%= image_tag "wide-skyscraper.jpg" %>
					</div>
					</div>
				</div>
	  </div>
</div>
		</div>
	 </div>
	</div>


