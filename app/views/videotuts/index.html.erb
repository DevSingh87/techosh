=begin
<%
  meta_title "#{@lecture.title} - MyCoolBlog"
  meta_description truncate(strip_tags(@lecture.description), :length => 200)  
  meta_og_url request.original_url
  meta_og_image "#{@lecture.image.image}"
  
%>
=end

	<div id="page-wrap">
	        <section id="tutorial">
	<div class="container">
		<div class="row">
			<div class="span7">
				<h4><%= @lecture.title %></h4>
				
			</div>
			
		</div>
	
</div>
</section>
		
			<div class="container-fluid">

				<div class="row-fluid">
				  <div class="span10">
				    <div class="post">
					                   <table id="videolist">
