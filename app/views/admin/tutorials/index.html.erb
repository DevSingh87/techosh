<h1>Listing tutorials</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tutorials.each do |tutorial| %>
  <tr>
    <td><%= tutorial.title %></td>
    <td><%= tutorial.description %></td>
    <td><%= link_to 'Show', [:admin, tutorial] %></td>
    <td><%= link_to 'Edit', edit_admin_tutorial_path(tutorial) %></td>
    <td><%= link_to 'Destroy', [:admin, tutorial], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tutorial', new_admin_tutorial_path %>
