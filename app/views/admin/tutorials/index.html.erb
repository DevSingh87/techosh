<div id="page-wrap" style="padding-top:10px;">
 <div class="container-fluid">
  <%= link_to 'New Tutorial', new_admin_tutorial_path, :class => 'btn btn-warning' %>
    <div class="row-fluid">
					<div class="span12">
						<div class="box box-color box-bordered">
							<div class="box-title">
								<h3>
									<i class="icon-table"></i>
									Listing Tutorials
								</h3>
							</div>
							<div class="box-content nopadding">
								<table class="table table-hover table-nomargin dataTable table-bordered">
									<thead>
										<tr>
											<th>Title</th>
											<th>Description</th>
											<th></th>
											<th></th>
											<th></th>
										  </tr>
									</thead>
									<tbody id="tutorials">
									 <%= render 'tutorials' %>
									</tbody>
								</table>
								<div class="table-pagination" id="paginator">
									<%= paginate @tutorials, :theme => 'twitter-bootstrap' %>
								</div>
							</div>
						</div>
					</div>
				</div>
				</div>
				</div>
				